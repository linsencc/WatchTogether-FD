{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "cookies"
  ],

  "host_permissions": [
    "http://127.0.0.1:5000/"
  ],

  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "/static/images/get_started16.png",
      "32": "/static/images/get_started32.png",
      "48": "/static/images/get_started48.png",
      "128": "/static/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/static/images/get_started16.png",
    "32": "/static/images/get_started32.png",
    "48": "/static/images/get_started48.png",
    "128": "/static/images/get_started128.png"
  },

  "content_scripts": [
    {
      "matches": ["https://*.bilibili.com/*", "https://www.mgtv.com/*"],
      "js": [
        "content/src/js/webpack/index.js"
      ]
    }
  ]
}